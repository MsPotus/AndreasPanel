<%= form_for @cohort,  remote: true do |f| %>
  <div class="field"> 
  <%= f.label :name %>:
  <%= f.text_field :name, id: :cohort_name %>
  </div>

 <div class="field"> 
  <%= f.label :start_date %>:
  <%= f.text_field :start_date, id: :cohort_start_date %>
  </div>

 <div class="field"> 
  <%= f.label :end_date %>:
  <%= f.text_field :end_date, id: :cohort_end_date %>
  </div>

 <div class="field"> 
  <%= f.label :course_id %>:
  <%= f.text_field :course_id, id: :cohort_course_id %>
  </div>

  <div class="action">
  <%= f.submit "Submit" %>
  </div>
<% end %>



