<%= form_for @student,  html: {class: "nifty_form"} do |f| %>
  <%= f.label :first_name %>:
  <%= f.text_field :first_name %>
  <%= f.label :last_name %>:
  <%= f.text_field :last_name%>
  <%= f.label :age %>:
  <%= f.text_field :age%>
  <%= f.label :highest_education %>:
  <%= f.text_field :highest_education%>
  <%= f.label :cohort_id %>:
  <%= f.text_field :cohort_id%>
  <%= f.submit "Create" %>
<% end %>


